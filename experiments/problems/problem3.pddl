(define (problem problem3) (:domain tusd)
(:objects
	train1 train2  - slt
	train3  - virm
	v1 t1 t2 t3 t4 t5 t6  - trackpart
	track1 track2  - track
)
(:init
	(entrance v1)
	(free t1)
	(free t2)
	(free t3)
	(free t4)
	(free t5)
	(free t6)
	(onTrack t1 track1)
	(onTrack t2 track1)
	(onTrack t3 track1)
	(onTrack t4 track2)
	(onTrack t5 track2)
	(onTrack t6 track2)
	(nextTo v1 t1) (nextTo t1 v1)
	(nextTo v1 t4) (nextTo t4 v1)
	(nextTo t1 t4) (nextTo t4 t1)
	(nextTo t1 t2) (nextTo t2 t1)
	(nextTo t2 t3) (nextTo t3 t2)
	(nextTo t4 t5) (nextTo t5 t4)
	(nextTo t5 t6) (nextTo t6 t5)
	(at train1 v1)
	(at train3 v1)
	(at train2 v1)
	(= (timestep) 0)
	(= (cost) 0)
	(= (arrive train1) 0)
	(= (arrive train2) 1)
	(= (arrive train3) 3)
	(= (departed train1) 0)
	(= (departed train2) 0)
	(= (departed train3) 0)
)
(:goal (and
	(exists (?t - slt) (= (departed ?t) 2))
	(exists (?t - slt) (= (departed ?t) 5))
	(exists (?t - virm) (= (departed ?t) 4))
	(forall (?t - trainunit) (and (hasBeenParked ?t) (at ?t v1)))
	(= (timestep) 6)
))
(
	:metric minimize (cost)
)
)